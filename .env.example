# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Supabase
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Supabase Dashboard
SUPABASE_DASHBOARD_USERNAME=admin
SUPABASE_DASHBOARD_PASSWORD=secure_password
POSTGRES_PASSWORD=your-database-password
JWT_SECRET=your-jwt-secret

# Redis
REDIS_URL=redis://redis:6379

# Stripe Configuration
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# Stripe Product Price IDs
STRIPE_CREATOR_PRICE_ID=price_creator
STRIPE_PRO_PRICE_ID=price_pro
STRIPE_ENTERPRISE_PRICE_ID=price_enterprise

# OAuth Credentials
SHOPIFY_CLIENT_ID=your-client-id
SHOPIFY_CLIENT_SECRET=your-client-secret
WOOCOMMERCE_CLIENT_ID=your-client-id
WOOCOMMERCE_CLIENT_SECRET=your-client-secret

# API Keys
RECAPTCHA_SITE_KEY=your-site-key
RECAPTCHA_SECRET_KEY=your-secret-key

# Next Auth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret # Generate with: openssl rand -base64 32

# Docker Specific
COMPOSE_PROJECT_NAME=print_vision_bolt

# Feature Flags
ENABLE_STORE_SYNC=true
ENABLE_BULK_OPERATIONS=true
ENABLE_EMAIL_SIGNUP=true
ENABLE_EMAIL_AUTOCONFIRM=true

# System Limits
MAX_ROWS=100000
JWT_EXPIRY=3600
